{
  "ecmaFeatures": {
    "arrowFunctions": true,
    "binaryLiterals": true,
    "blockBindings": true,
    "classes": false,
    "defaultParams": true,
    "forOf": true,
    "generators": true,
    "modules": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralDuplicateProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "octalLiterals": true,
    "regexUFlag": true,
    "regexYFlag": true,
    "restParams": true,
    "spread": true,
    "superInFunctions": false,
    "templateStrings": true,
    "unicodeCodePointEscapes": true,
    "globalReturn": true,
    "jsx": false
  },

  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true,
    "mocha": true,
    "node": true
  },

  "globals": {},

  "plugins": [],

  "rules": {
    "comma-dangle": [2, "never"],
    "no-cond-assign": [2, "always"],
    "no-console": 2,
    "no-constant-condition": 2,
    "no-control-regex": 2,
    "no-debugger": 2,
    "no-dupe-args": 2,
    "no-dupe-keys": 2,
    "no-duplicate-case": 2,
    "no-empty": 2,
    "no-ex-assign": 2,
    "no-extra-boolean-cast": 2,
    "no-extra-parens": [1, "all"],
    "no-extra-semi": 2,
    "no-func-assign": 2,
    "no-inner-declarations": [2, "functions"],
    "no-invalid-regexp": 2,
    "no-irregular-whitespace": 2,
    "no-negated-in-lhs": 2,
    "no-obj-calls": 2,
    "no-regex-spaces": 2,
    "no-reserved-keys": 2,
    "no-sparse-arrays": 2,
    "no-unreachable": 2,
    "use-isnan": 2,
    "valid-jsdoc": 1,
    "valid-typeof": 2,
    "no-unexpected-multiline": 2,

    "accessor-pairs": 1,
    "block-scoped-var": 2,
    "complexity": 0,
    "consistent-return": 2,
    "curly": [2, "all"],
    "default-case": 1,
    "dot-notation": 0,
    "dot-location": [2, "object"],
    "eqeqeq": [2, "smart"],
    "guard-for-in": 1,
    "no-alert": 2,
    "no-caller": 2,
    "no-div-regex": 2,
    "no-else-return": 1,
    "no-empty-label": 2,
    "no-eq-null": 0,
    "no-eval": 2,
    "no-extend-native": 2,
    "no-extra-bind": 2,
    "no-fallthrough": 1,
    "no-floating-decimal": 2,
    "no-implicit-coercion": 2,
    "no-implied-eval": 2,
    "no-invalid-this": 2,
    "no-iterator": 2,
    "no-labels": 2,
    "no-lone-blocks": 2,
    "no-loop-func": 1,
    "no-multi-spaces": 2,
    "no-multi-str": 2,
    "no-native-reassign": 2,
    "no-new-func": 2,
    "no-new-wrappers": 2,
    "no-new": 2,
    "no-octal-escape": 2,
    "no-octal": 2,
    "no-param-reassign": [2, { "props": true }],
    "no-process-env": 1,
    "no-proto": 2,
    "no-redeclare": [1, { "builtinGlobals": true }],
    "no-return-assign": [2, "always"],
    "no-script-url": 2,
    "no-self-compare": 2,
    "no-sequences": 2,
    "no-throw-literal": 2,
    "no-unused-expressions": [2, { "allowShortCircuit": true, "allowTernary": true }],
    "no-useless-call": 1,
    "no-useless-concat": 1,
    "no-void": 1,
    "no-warning-comments": [1, { "terms": ["todo", "fixme"], "location": "anywhere" }],
    "no-with": 2,
    "radix": 2,
    "vars-on-top": 1,
    "wrap-iife": [2, "any"],
    "yoda": 0,

    "strict": [1, "function"],

    "init-declarations": 0,
    "no-catch-shadow": 2,
    "no-delete-var": 2,
    "no-label-var": 2,
    "no-shadow-restricted-names": 2,
    "no-shadow": [2, { "builtinGlobals": true, "hoist": "all" }],
    "no-undef-init": 2,
    "no-undef": 2,
    "no-undefined": 2,
    "no-unused-vars": 2,
    "no-use-before-define": 2,

    "callback-return": [1, ["callback", "cb", "next"]],
    "global-require": 2,
    "handle-callback-err": 2,
    "no-mixed-requires": 1,
    "no-new-require": 2,
    "no-path-concat": 2,
    "no-process-exit": 1,
    "no-restricted-modules": 0,
    "no-sync": 0,

    "array-bracket-spacing": [2, "always"],
    "block-spacing": [2, "always"],
    "brace-style": [2, "1tbs", { "allowSingleLine": true }],
    "camelcase": [2, { "properties": "always" }],
    "comma-spacing": [2, { "before": false, "after": true }],
    "comma-style": [2, "last"],
    "computed-property-spacing": [2, "never"],
    "consistent-this": 1,
    "eol-last": 2,
    "func-names": 1,
    "func-style": 0,
    "id-length": [1, { "min": 2, "properties": "always", "exceptions": ["i", "j"] }],
    "id-match": 0,
    "indent": [2, 2],
    "jsx-quotes": 0,
    "key-spacing": [2, { "beforeColon": false, "afterColon": true }],
    "lines-around-comment": [1, { "beforeBlockComment": true, "afterBlockComment": false, "beforeLineComment": true, "afterLineComment": false }],
    "linebreak-style": [2, "unix"],
    "max-nested-callbacks": [1, 3],
    "new-cap": 1,
    "new-parens": 2,
    "newline-after-var": [2, "always"],
    "no-array-constructor": 2,
    "no-continue": 1,
    "no-inline-comments": 1,
    "no-lonely-if": 1,
    "no-mixed-spaces-and-tabs": 2,
    "no-multiple-empty-lines": [2, { "max": 2 }],
    "no-nested-ternary": 2,
    "no-negated-condition": 1,
    "no-new-object": 2,
    "no-restricted-syntax": 0,
    "no-spaced-func": 2,
    "no-ternary": 0,
    "no-trailing-spaces": 2,
    "no-underscore-dangle": 1,
    "no-unneeded-ternary": 2,
    "object-curly-spacing": [2, "always"],
    "one-var": 0,
    "operator-assignment": 0,
    "operator-linebreak": [2, "after"],
    "padded-blocks": [2, "never"],
    "quote-props": 0,
    "quotes": [2, "single", "avoid-escape"],
    "require-jsdoc": 0,
    "semi-spacing": [2, {"before": false, "after": true}],
    "semi": [2, "always"],
    "sort-vars": 0,
    "space-after-keywords": [2, "always"],
    "space-before-keywords": [2, "always"],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, {"anonymous": "never", "named": "always"}],
    "space-in-parens": [2, "always"],
    "space-infix-ops": [2, { "int32Hint": false }],
    "space-return-throw-case": 2,
    "space-unary-ops": [1, { "words": true, "nonwords": false }],
    "spaced-line-comment": [2, "always", { "exceptions": ["-", "+", "/"] }],
    "wrap-regex": 1,

    "arrow-parens": [2, "always"],
    "arrow-spacing": [2, { "before": true, "after": true }],
    "constructor-super": 0,
    "generator-star-spacing": [2, { "before": false, "after": true }],
    "no-class-assign": 0,
    "no-const-assign": 2,
    "no-dupe-class-members": 0,
    "no-this-before-super": 0,
    "no-var": 0,
    "object-shorthand": 0,
    "prefer-arrow-callback": 0,
    "prefer-const": 0,
    "prefer-spread": 0,
    "prefer-reflect": 0,
    "prefer-template": 0,
    "require-yield": 2,

    "max-len": [2, 100, { "ignoreComments": true, "ignoreUrls": true }],
    "no-bitwise": 2,
    "no-plusplus": 2
  },

  "settings": {},

  "extends": {}
}
